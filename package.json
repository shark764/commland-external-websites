{
  "name": "@commland/external-websites",
  "private": true,
  "scripts": {
    "start": "webpack serve",
    "start:standalone": "webpack serve --env standalone",
    "build": "webpack --mode=production",
    "build:commland": "yarn build && cp -TRrf ./dist ../core/applications/external-websites",
    "analyze": "webpack --mode=production --env analyze",
    "check-format": "prettier --check .",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "typescript": "tsc --noEmit --emitDeclarationOnly false",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|html)\"",
    "prettify": "yarn run format && yarn run lint:fix",
    "full-check": "yarn run prettify && yarn run typescript",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "devDependencies": {
    "@2600hz/commio-native-utilities": "^0.5.2",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.181",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/systemjs": "^6.1.1",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "sass": "^1.34.1",
    "sass-loader": "^12.3.0",
    "single-spa-react": "^4.6.0",
    "style-loader": "^3.3.1",
    "ts-config-single-spa": "^3.0.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.1",
    "webpack-config-single-spa-react": "^4.0.3",
    "webpack-config-single-spa-react-ts": "^4.0.3",
    "webpack-config-single-spa-ts": "^4.1.2",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@2600hz/sds-core": "^1.1.0",
    "@2600hz/sds-react-components": "^1.33.4",
    "@2600hz/sds-themes": "^1.1.3",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "11.14.3",
    "react-router-dom": "6.0.2",
    "zustand": "^3.7.2"
  }
}
